{
  "name": "proreactnativetemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "pod": "npx pod-install ios",
    "android:stage": "react-native run-android --variant=stageDebug --appIdSuffix=stage",
    "android:stage-release": "react-native run-android --variant=stageRelease  --appIdSuffix=stage",
    "android:dev": "react-native run-android --variant=devDebug  --appIdSuffix=dev",
    "android:dev-release": "react-native run-android --variant=devRelease  --appIdSuffix=dev",
    "android:prod": "react-native run-android --variant=prodDebug  --appIdSuffix=prod",
    "android:prod-release": "react-native run-android --variant=prodRelease  --appIdSuffix=prod",
    "android:devBuild":"cd android && ./gradlew bundleDevRelease"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.4.1",
    "@react-navigation/native": "^6.0.14",
    "@react-navigation/native-stack": "^6.9.2",
    "axios": "^1.2.0",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-config": "^1.4.11",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
